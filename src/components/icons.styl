@font-face
  font-family 'icons'
  src url('assets/font/icons.eot')
  src url('assets/font/icons.eot?#iefix') format('eot'),
    url('assets/font/icons.woff') format('woff'),
    url('assets/font/icons.woff2') format('woff2'),
    url('assets/font/icons.ttf') format('truetype')
  font-weight normal
  font-style normal

icon(type)
  if type
    content type
  display inline-block
  line-height 1
  vertical-align middle
  margin-top -2px
  font-family 'icons'
  font-style normal
  font-weight normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

$icon-add = '\EA01'
.icon-add:before
  icon($icon-add)

$icon-arrow-down = '\EA02'
.icon-arrow-down:before
  icon($icon-arrow-down)

$icon-calendar = '\EA03'
.icon-calendar:before
  icon($icon-calendar)

$icon-chevron-left = '\EA04'
.icon-chevron-left:before
  icon($icon-chevron-left)

$icon-chevron-right = '\EA05'
.icon-chevron-right:before
  icon($icon-chevron-right)

$icon-clock = '\EA06'
.icon-clock:before
  icon($icon-clock)

$icon-delete = '\EA07'
.icon-delete:before
  icon($icon-delete)

$icon-list = '\EA08'
.icon-list:before
  icon($icon-list)

$icon-star = '\EA09'
.icon-star:before
  icon($icon-star)

$icon-trash = '\EA0A'
.icon-trash:before
  icon($icon-trash)
